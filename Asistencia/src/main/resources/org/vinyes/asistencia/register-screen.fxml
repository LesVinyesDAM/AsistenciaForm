<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.vinyes.asistencia.Controllers.RegisterController" prefHeight="250.0" prefWidth="350.0">
    <TextField fx:id="onPassCardNfc" promptText="Pase una tarjeta..." editable="false" layoutX="20" layoutY="20" prefWidth="200"/>
    <TextField fx:id="nameField" promptText="Nombre/s" layoutX="20" layoutY="70" prefWidth="200"/>
    <TextField fx:id="surnameField" promptText="Apellido/s" layoutX="20" layoutY="120" prefWidth="200"/>
    <TextField fx:id="departmentField" promptText="Departmento" layoutX="20" layoutY="170" prefWidth="200"/>
    <Button text="Cancelar" fx:id="cancelButton" layoutX="120" layoutY="220" onAction="#handleCancel"/>
    <Button text="Registrar" fx:id="registerButton" layoutX="20" layoutY="220" onAction="#handleRegister"/>
</AnchorPane>
